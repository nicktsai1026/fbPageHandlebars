<div class="container-fluid">
    <h1 align='center'>You have {{pageCounter}} pages in common  <br>  with {{name}}!</h1>
</div>
<div class="container-fluid">
    <div class="list-group">
        <div class="list-group-item ">
            <a href="/checkFriends">
                <span class="glyphicon glyphicon-chevron-left"></span>Back to friends
            </a>
        </div>
    {{#commonPages}}
        <div class="list-group-item">
            <span class="pageItem">
                <span class="list-group-item-text badge">{{fan_count}} Likes</span>
            </span>
            <a href="{{link}}" target="_blank">
                <img class="pageImg" src="{{picture.data.url}}" alt="picture">
                <h3 class="list-group-item-heading">{{name}}</h3>
                <span class="list-group-item-text">About: {{about}}</span>
            </a>
        </div>
    {{/commonPages}}
        <div class="list-group-item ">
            <a href="/checkFriends">
                <span class="glyphicon glyphicon-chevron-left"></span>Back to friends
            </a>
        </div>
    </div>
</div>
